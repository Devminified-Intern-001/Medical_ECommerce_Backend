{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "build/**",
                    "docs/**"
                ]
            }
        },
        {
            "src": "dist/index.html",
            "use": "@vercel/static",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "build/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.html"
        }
    ]
}